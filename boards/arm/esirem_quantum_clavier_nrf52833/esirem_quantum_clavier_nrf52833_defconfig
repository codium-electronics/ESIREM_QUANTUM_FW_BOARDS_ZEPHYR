CONFIG_SOC_SERIES_NRF52X=y
CONFIG_SOC_NRF52833_QIAA=y
CONFIG_BOARD_ESIREM_QUANTUM_CLAVIER_NRF52833=y

# Enable MPU
CONFIG_ARM_MPU=y

# Enable the UART driver
CONFIG_SERIAL=y

# enable GPIO
CONFIG_GPIO=y

# disable console (no UART left, one for the PIC, one for the fibocomi LTE modem)
CONFIG_CONSOLE=y
CONFIG_UART_CONSOLE=n

# additional board options
CONFIG_GPIO_AS_PINRESET=y
